{
  "name": "ui-lib",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "compile": "babel components --out-dir dist --copy-files",
    "format": "prettier '**/*.{js,jsx}' --write",
    "eslint": "eslint './*.{js,jsx}' --ext jsx --ext js --fix --max-warnings=0",
    "postcompile": "git add dist",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-react": "^5.0.6",
    "@iconify/react": "^4.1.1",
    "ckeditor5-custom-build": "file:./ckeditor5",
    "next": "12.3.1",
    "next-images": "^1.8.4",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.6",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@ckeditor/ckeditor5-dev-utils": "^32.0.2",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "chromatic": "^11.0.8",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.1.8",
    "yarn": "^1.22.19"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "yarn format",
      "yarn compile"
    ]
  },
  "ignore": [
    "node_modules",
    "*.config.js"
  ]
}
